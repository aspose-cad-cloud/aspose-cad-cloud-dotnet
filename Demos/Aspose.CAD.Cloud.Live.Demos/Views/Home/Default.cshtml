@model LandingPageModel

@{ ViewBag.Title = "Default"; }

<div class="container-fluid header1 header2 minify-header appfamily-header">
    <div class="container">
        <div class="row">
            <div class="col-md-8">
                <h1>Build CAD Converter via C# REST SDK</h1>
                <h4>C# Cloud SDK to help you convert CAD documents stored on the cloud.</h4>
            </div>
            <div class="col-md-4 tc">
                <img class="col-xs-12" src="../img/aspose_cad-for-cloud.png" alt="Aspose.CAD Cloud" width="320" />
            </div>
        </div>
    </div>
</div>
<div class="container-fluid productfamilypage bg-white pdb-5">
    <div class="row">
        <div class="container tc">
            <div class="clearfix">&nbsp;</div>
            <div class="clearfix">&nbsp;</div>
            <h3>Aspose.CAD Cloud .NET APIs Includes</h3>
            <div class="clearfix">&nbsp;</div>
            <div class="clearfix">&nbsp;</div>

            <div class="col-lg-4 productfamily_box">
                <div class="productfamilytitle">
                    <div class="imgblock">
                        <img src="../img/aspose_conversion-app.png" alt="Aspose.CAD Cloud .NET SDK" />
                    </div>
                    <a href="/cad/conversion">
                        <span class="spanclass">Aspose.CAD @Model.Resources["AsposeProductFor"] Cloud<em>Conversion</em></span>
                        <em>Easily convert CAD documents i.e. DXF, DWG, DGN, DWF, DWFX, IFC, STL, DWT, IGES, PLT, CF2, OBJ, HPGL or IGS drawing files to PDF and Image formats.</em>
                    </a>
                </div>
            </div>
        </div>
    </div>
</div>
<script type="text/javascript">
    window.onload = function () {
        alert('Please set "Client Id" and "Client Secret" in appsettings.json to run Aspose.CAD Cloud .NET APIs');
    };
</script>
